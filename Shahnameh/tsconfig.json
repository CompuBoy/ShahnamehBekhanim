{
  "compilerOptions": {
    "target": "es5",
    "strict": true,
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "sourceMap": false,
    "jsx": "react",
    "jsxFactory": "h",
    "lib": ["es2015", "dom"],
    "allowSyntheticDefaultImports": true,
  },
  "exclude": ["node_modules"]
}
